function alternateSort(arr, n)
{
    console.log(arr);
    arr.sort(function (a, b)
    {
        return a - b;
    });
    console.log(arr);
    var i = 0,
    j = n - 1;
    while(i<j)
    {
        document.write(arr[j--] + "");
        document.write(arr[i++] + "");
    }
    if (n % 2 != 0) document.write(arr[i]);
}
var arr = [12, 74, 4, 27, 94, 21, 65, 17, 2];
var n = arr.length;
alternateSort(arr, n)
